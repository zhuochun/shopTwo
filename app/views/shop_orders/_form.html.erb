<%= simple_form_for(@shop_order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :store %>
    <%= f.input :quantity %>
    <%= f.input :subtotal %>
    <%= f.input :credits %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
